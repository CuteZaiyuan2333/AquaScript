# 错误处理测试

func divide(a: int, b: int) -> int:
    if b == 0:
        print("Error: Division by zero!")
        return 0
    else:
        return a / b

func test_list_access():
    var numbers = [1, 2, 3]
    print("Valid access:", numbers[1])
    # 这里会测试边界检查
    print("Length:", len(numbers))

# 测试除零
var result1: int = divide(10, 2)
print("10 / 2 =", result1)

var result2: int = divide(10, 0)
print("10 / 0 =", result2)

# 测试列表访问
test_list_access()

# 测试类型转换
var num_str: str = "123"
var num_int: int = int(num_str)
print("String to int:", num_int)

# 测试字符串操作
var text: str = "Hello World"
print("Text:", text)
print("Length:", len(text))
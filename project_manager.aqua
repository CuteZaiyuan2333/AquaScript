func assess_project_risk(blocked_tasks, total_tasks, progress):
    risk_score = 0
    
    # é˜»å¡ä»»åŠ¡é£é™©
    if blocked_tasks > 0:
        risk_score = risk_score + blocked_tasks * 2
    
    # è¿›åº¦é£é™©
    if progress < 25:
        risk_score = risk_score + 3
    elif progress < 50:
        risk_score = risk_score + 2
    elif progress < 75:
        risk_score = risk_score + 1
    
    return risk_score


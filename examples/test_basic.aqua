# 基本测试

func test_list():
    var numbers = [1, 2, 3, 4, 5]
    print("数字列表: " + str(numbers))
    print("列表长度: " + str(len(numbers)))
    return True

func test_dict():
    var student = {"name": "张三", "age": 20}
    print("学生信息: " + str(student))
    print("学生姓名: " + student["name"])
    return True

func main():
    print("基本功能测试")
    test_list()
    test_dict()

main()